 *
 * Notes: Keymap for 2% Milk Keyboard
 * Top Key: Play/Pause
 * Bottom Key: Next
 */

#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp C_NEXT
                &kp C_PLAY_PAUSE
            >;
        };
    };
};